<?xml version="1.0" encoding="utf-8"?>

<!-- this code is used to customize the about icon style -->

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	
	<!-- when about icon pressed -->
    <item android:state_pressed="true"
        android:drawable="@drawable/border_shadow_back_next_back_unpress" />

    <!-- when about icon in normal -->
    <item android:drawable="@drawable/border_shadow_back_next_back_press" />
</selector>